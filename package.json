{
  "name": "bunway",
  "version": "1.0.0",
  "description": "Express-style routing toolkit built natively for Bun.",
  "private": true,
  "type": "module",
  "module": "./src/index.ts",
  "license": "MIT",
  "author": "JointOps <shayanhussain48@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/JointOps/bunway.git"
  },
  "bugs": {
    "url": "https://github.com/JointOps/bunway/issues"
  },
  "homepage": "https://bunway.jointops.dev",
  "keywords": [
    "bun",
    "router",
    "express",
    "expressjs",
    "express-style",
    "middleware",
    "http",
    "rest",
    "web-framework",
    "server",
    "typescript",
    "jointops",
    "bun-express",
    "bun-middleware",
    "bun-http",
    "bun-routing",
    "fetch-router",
    "express-middleware",
    "bun-serve",
    "bun-framework"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:acceptance": "bun test tests/acceptance",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch",
    "benchmark": "bun benchmark/run.ts",
    "benchmark:quick": "bun benchmark/run.ts --quick",
    "benchmark:ci": "bun benchmark/run.ts --ci --bunway-only",
    "benchmark:fair": "bun benchmark/fair-bench.ts",
    "benchmark:fair:quick": "bun benchmark/fair-bench.ts --quick",
    "benchmark:setup": "./benchmark/scripts/setup.sh",
    "bench": "bun benchmark/servers/bunway.ts",
    "bench:compare": "./benchmark/scripts/run-comparison.sh",
    "bench:quick": "./benchmark/scripts/quick-bench.sh",
    "build": "tsc --project tsconfig.json",
    "docs": "typedoc --options typedoc.json",
    "docs:dev": "npm run docs && vitepress dev docs",
    "docs:build": "npm run docs && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "format": "prettier --write .",
    "sync:version": "node scripts/sync-version.mjs",
    "prepare:dist": "rm -rf dist && npm run build && node scripts/prepare-dist.mjs",
    "check:dist": "node scripts/verify-dist.mjs",
    "release": "npm run typecheck && npm run test && npm run docs:build && npm run prepare:dist && npm run check:dist && npm publish dist --access public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "autocannon": "^8.0.0",
    "elysia": "^1.4.22",
    "express": "^5.2.1",
    "fastify": "^5.7.2",
    "hono": "^4.11.7",
    "prettier": "^3.8.1",
    "typedoc": "^0.28.16",
    "vitepress": "^1.6.4"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  }
}
